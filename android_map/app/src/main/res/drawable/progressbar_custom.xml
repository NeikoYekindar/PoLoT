<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="0"
    android:toDegrees="360"
    android:pivotX="50%"
    android:pivotY="50%">
    <shape
        android:shape="ring"
        android:useLevel="false">
        <size
            android:width="48dp"
            android:height="48dp" />
        <gradient
            android:startColor="#D9D6D6"
            android:endColor="#256B6A"
            android:angle="0"
            android:type="sweep"
            android:useLevel="false" />
    </shape>
</rotate>
